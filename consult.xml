<?xml version='1.0' ?>

<t:chloe xmlns:t="http://factorcode.org/chloe/1.0">

	<t:title>VelibWatch -- Consultation</t:title>

	<table class="report-list">

		<tr>
			<th>station</th>
			<th>type</th>
			<th>username</th>
		</tr>

		<t:bind-each t:name="reports">

			<tr>
				<td>
					<t:label t:name="station" />
				</td>
				<td>
					<t:label t:name="type" />
				</td>
				<td>
					<t:label t:name="username" />
				</td>
			</tr>

		</t:bind-each>

	</table>

</t:chloe>
